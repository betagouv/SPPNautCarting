	<xs:element name="S100_ExchangeCatalogue" type="S100_ExchangeCatalogue"/>
	<xs:complexType name="S100_ExchangeCatalogue">
		<xs:annotation>
			<xs:documentation>Each exchange set has a single S-100_ExchangeCatalogue which contains meta information for the data and support files in the exchange set. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="S100_CatalogueIdentifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Uniquely identifies this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="S100_CataloguePointofContact" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the issuer of this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the product specifications used for the datasets contained in the exchange catalogue. Conditional on all the datasets using the same product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataLanguage" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the language.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueName" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Catalogue filename.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueDescription" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of what the exchange catalogue contains</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueComment" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Any additional Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compressionFlag" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Is the data compressed? Yes or No</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sourceMedia" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Distribution media</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="replacedData" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>If a data file is canceled is it replaced by another data file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataReplacement" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cell name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="datasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>-->
			<!--<xs:element ref="datasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Exchange catalogues may include or reference discovery metadata for the datasets in the exchange set</xs:documentation>
				</xs:annotation>
			</xs:element>-->
			<xs:element name="datasetDiscoveryMetadata">
				<xs:annotation>
					<xs:documentation>Container for S100_DatasetDiscoveryMetadata elements</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="S100_DatasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"><xs:annotation><xs:documentation>Discovery metadata for single dataset. Constraints should be in a separate file for reusability and potential customization.</xs:documentation></xs:annotation></xs:element>
						<!--<xs:element name="S100_DatasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation><xs:documentation>Discovery metadata for single dataset. Constraints should be in a separate file for reusability and potential customization.</xs:documentation></xs:annotation>
						</xs:element>-->
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="supportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="S100_CatalogueMetadata" type="S100_CatalogueMetadata" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Metadata for feature and portrayal catalogues (Fig. 4a-D-2 suggests their metadata should be in the exchange catalogue, 4a-D-3 suggests their metadata might be separate files in the exchange set)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_CatalogueMetadata" type="S100_CatalogueMetadata"><xs:annotation><xs:documentation>Substitution group head element</xs:documentation></xs:annotation></xs:element>
	<xs:complexType name="S100_CatalogueMetadata">
