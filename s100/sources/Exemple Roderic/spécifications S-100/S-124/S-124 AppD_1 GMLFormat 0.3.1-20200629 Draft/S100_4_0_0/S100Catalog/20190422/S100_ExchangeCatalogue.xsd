	<xs:import namespace="http://standards.iso.org/iso/19115/-3/cit/2.0" schemaLocation="../../../../standards.iso.org/19115/-3/cit/2.0/cit.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/gco/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/gco/1.0/gco.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/gcx/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/gcx/1.0/gcx.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/gex/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/gex/1.0/gex.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/lan/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/lan/1.0/lan.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/mco/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/mco/1.0/mco.xsd"/>
	<xs:import namespace="http://standards.iso.org/iso/19115/-3/mri/1.0" schemaLocation="../../../../standards.iso.org/19115/-3/mri/1.0/mri.xsd"/>
	<xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="http://www.w3.org/1999/xlink.xsd"/>

	<xs:element name="S100_ProductSpecification" type="S100_ProductSpecification"/>
	<xs:complexType name="S100_ProductSpecification">
		<xs:annotation>
			<xs:documentation>The Product Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The name of the product specification used to create the datasets.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="number_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The number assigned to the product specification in the Product Specification register</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_SupportFileSpecification" type="S100_SupportFileSpecification"/>
	<xs:complexType name="S100_SupportFileSpecification">
		<xs:annotation>
			<xs:documentation>The Support File Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The name of the specification used to create the support file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="NillableString_PropertyType" minOccurs="0" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="S100_SupportFilePurpose">
		<xs:annotation>
			<xs:documentation>The reason for inclusion of the support file in this exchange set</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="new">
				<xs:annotation>
					<xs:documentation>A file which is new. Signifies a new file.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="replacement">
				<xs:annotation>
					<xs:documentation>A file which replaces an existing file. Signifies a replacement for a file of the same name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="deletion">
				<xs:annotation>
					<xs:documentation>Deletes an existing file. Signifies deletion of a file of that name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--<xs:simpleType name="PositiveIntegerorNil">
		<xs:annotation>
			<xs:documentation>Positive Integer or Nilled. Needed because S-101 1.0.0 12.1.2.3 allows nilling minimum display scale.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:positiveInteger">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>-->

	<xs:element name="S100_SupportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata" />
	<xs:complexType name="S100_SupportFileDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>Metadata about the individual support files in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>File name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileLocation" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Full path from the exchange set root directory. Path relative to the root directory of the exchange set. The location of the file after the exchange set is unpacked into directory &lt;EXCH_ROOT> will be &lt;EXCH_ROOT>/&lt;filePath>/&lt;filename></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="S100_SupportFilePurpose" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the dataset has been issued. e.g. new, re-issue, new edition, update etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The edition number of the dataset. When a data set is initially created, the edition number 1 is assigned to it. The edition number is increased by 1 at each new edition. Edition number remains the same for a re-issue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supportFileSpecification" type="S100_SupportFileSpecification" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The product specification used to create this file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataType" type="S100_SupportFileFormat" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The format of the support file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherDataTypeDescription" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Encoding format other than those listed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional comment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="S100_DigitalSignature" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="S100_DigitalSignatureValue" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultLocale" type="lan:PT_Locale_PropertyType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="S100_DigitalSignatureValue" mixed="true">
		<xs:sequence>
			<xs:element name="signedPublicKey">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:base64Binary">
							<xs:attribute name="id" type="xs:string" use="required"/>
							<xs:attribute name="rootKey" type="xs:string" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="digitalSignature">
				<xs:simpleType>
					<xs:restriction base="xs:hexBinary">
						<xs:maxLength value="100"><xs:annotation><xs:documentation>up to 100 octets</xs:documentation></xs:annotation></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_CatalogueIdentifier" type="S100_CatalogueIdentifier"/>
	<xs:complexType name="S100_CatalogueIdentifier">
		<xs:annotation>
			<xs:documentation>An exchange catalogue contains the discovery metadata about the exchange datasets and support files</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Uniquely identifies this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The edition number of this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Creation date of the exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="S100_CataloguePointofContact">
		<xs:annotation>
			<xs:documentation>Contact details of the issuer of this exchange catalogue</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="organization" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The organization distributing this exchange catalogue. This could be an individual producer, value added reseller, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phone" type="S100_Phone_Type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="address" type="S100_Address_Type" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="S100_DatasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata"/>
	<xs:complexType name="S100_DatasetDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>Metadata about the individual datasets in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Dataset file name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="filePath" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Full path from the exchange set root directory</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="NillableString_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Short description giving the area or location covered by the dataset. e.g. a harbour or port name, between two named locations etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataProtection" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the data is encrypted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="protectionScheme" type="S100_ProtectionScheme" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For example S-63</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="S100_DigitalSignature_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file. Specifies the algorithm used to compute digitalSignatureValue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="S100_DigitalSignatureValue_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature. The value resulting from application of digitalSignatureReference</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyright" type="S100_RestrictionCode_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the dataset is copyrighted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classification" type="S100_ClassificationCode_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates the security classification of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the dataset has been issued. e.g. new, re-issue, new edition, update etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specificUsage" type="S100_Usage_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The use for which the dataset is intended. e.g. in the case of ENCs this would be a navigation purpose classification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of the dataset. When a data set is initially created, the edition number 1 is assigned to it. The edition number is increased by 1 at each new edition. Edition number remains the same for a re-issue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Update number assigned to the dataset and increased by one for each subsequent update. Update number 0 is assigned to a new dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateApplicationDate" type="gco:Date_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This date is only used for the base cell files (i.e. new data sets, re-issue and new edition), not update cell files. All updates dated on or before this date must have been applied by the producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueTime" type="xs:time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Time of day at which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The product specification used to create this dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="producingAgency" type="cit:CI_Responsibility_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ProducingAgency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="optimumDisplayScale" type="xs:positiveInteger" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The modulus of the display scale. Example: A scale of 1:25000 is encoded as 25000.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumDisplayScale" type="xs:positiveInteger" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The maximum scale with which the data is displayed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumDisplayScale" type="xs:positiveInteger" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The minimum scale with which the data is displayed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="horizontalDatumReference" type="horizontalDatumReference_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Reference to the register from which the horizontal datum value is taken. e.g.EPSG</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="horizontalDatumValue" type="horizontalDatumValue_PropertyType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Horizontal Datum of the entire dataset. e.g. EPSG 4326</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="epoch" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="verticalDatum" type="S100_VerticalAndSoundingDatum_PropertyType" minOccurs="0" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Vertical Datum of the entire dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="soundingDatum" type="S100_VerticalAndSoundingDatum_PropertyType" minOccurs="0" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Sounding Datum of the entire dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
