    <xs:complexType name="categoryOfAuthorityType">
        <xs:annotation>
            <xs:documentation>The type of person, government agency or organisation granted powers of managing or controlling access to and/or activity in an area.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="categoryOfAuthorityLabel">
                <xs:attribute name="code" type="categoryOfAuthorityCode" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="categoryOfAuthorityLabel">
        <xs:annotation>
            <xs:documentation>The type of person, government agency or organisation granted powers of managing or controlling access to and/or activity in an area.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"><xs:enumeration value="Border Control"><xs:annotation><xs:documentation>2: The administration to prevent or detect and prosecute violations of rules and regulations at international boundaries.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Police"><xs:annotation><xs:documentation>3: The department of government, or civil force, charged with maintaining public order.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Port"><xs:annotation><xs:documentation>4: Person or corporation, owners of, or entrusted with or invested with the power of managing a port. May be called a Harbour Board, Port Trust, Port Commission, Harbour Commission, Marine Department.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Immigration"><xs:annotation><xs:documentation>5: The authority controlling people entering a country.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Health"><xs:annotation><xs:documentation>6: The authority with responsibility for checking the validity of the health declaration of a vessel and for declaring free pratique.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Coast Guard"><xs:annotation><xs:documentation>7: Organization keeping watch on shipping and coastal waters according to governmental law; normally the authority with responsibility for search and rescue.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Agricultural"><xs:annotation><xs:documentation>8: The authority with responsibility for preventing infection of the agriculture of a country and for the protection of the agricultural interests of a country.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Military"><xs:annotation><xs:documentation>9: A military authority which provides control of access to or approval for transit through designated areas or airspace.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Private Company"><xs:annotation><xs:documentation>10: A private or publicly owned company or commercial enterprise which exercises control of facilities, for example a calibration area.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Maritime Police"><xs:annotation><xs:documentation>11: A governmental or military force with jurisdiction in territorial waters. Examples could include Gendarmerie Maritime, Carabinierie, and Guardia Civil.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Environmental"><xs:annotation><xs:documentation>12: An authority with responsibility for the protection of the environment.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Fishery"><xs:annotation><xs:documentation>13: An authority with responsibility for the control of fisheries.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Finance"><xs:annotation><xs:documentation>14: An authority with responsibility for the control and movement of money.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Maritime"><xs:annotation><xs:documentation>15: A national or regional authority charged with administration of maritime affairs.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Customs"><xs:annotation><xs:documentation>16: The agency or establishment for collecting duties, tolls.</xs:documentation></xs:annotation></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
