            <xs:element name="development" type="developmentType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="locationByText" type="locationByTextType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="textContent" type="textContentType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="contactAddressType">
        <xs:annotation>
            <xs:documentation>Direction or superscription of a letter, package, etc., specifying the name of the place to which it is directed, and optionally a contact person or organisation who should receive it.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="deliveryPoint" type="deliveryPointType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="cityName" type="cityNameType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="administrativeDivision" type="administrativeDivisionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="countryName" type="countryNameType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="postalCode" type="postalCodeType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="depthsDescriptionType">
        <xs:annotation>
            <xs:documentation>Textual description of the characteristics and notable matters pertaining to depths in an area.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="categoryOfDepthsDescription" type="categoryOfDepthsDescriptionType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="facilitiesLayoutDescriptionType">
        <xs:annotation>
            <xs:documentation>Textual description of the layout of port facilities.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="featureNameType">
        <xs:annotation>
            <xs:documentation>Provides the name of an entity, defines the national language of the name, and provides the option to display the name at various system display settings.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="displayName" type="displayNameType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="language" type="languageType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="name" type="nameType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="fixedDateRangeType">
        <xs:annotation>
            <xs:documentation>The complex attribute describes single fixed period, as the date range between its sub-attributes.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="dateStart" type="dateStartType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="dateEnd" type="dateEndType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="frequencyPairType">
        <xs:annotation>
            <xs:documentation>A pair of frequencies for transmitting and receiving radio signals. The shore station transmits and receives on the frequencies indicated.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="frequencyShoreStationTransmits" type="frequencyShoreStationTransmitsType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="frequencyShoreStationReceives" type="frequencyShoreStationReceivesType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="contactInstructions" type="contactInstructionsType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="generalHarbourInformationType">
        <xs:annotation>
            <xs:documentation>General information about the port or harbour area.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="generalPortDescription" type="generalPortDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="facilitiesLayoutDescription" type="facilitiesLayoutDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="limitsDescription" type="limitsDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="constructionInformation" type="constructionInformationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="cargoServicesDescription" type="cargoServicesDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="weatherResource" type="weatherResourceType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="generalPortDescriptionType">
        <xs:annotation>
            <xs:documentation>General, introductory information about the port.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="graphicType">
        <xs:annotation>
            <xs:documentation>Pictorial information such as a photograph, sketch or other graphic, optionally accompanied by descriptive information about the graphic and the location relative to its subject from which it was made.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="pictorialRepresentation" type="pictorialRepresentationType" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="pictureCaption" type="pictureCaptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="sourceDate" type="sourceDateType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pictureInformation" type="pictureInformationType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="bearingInformation" type="bearingInformationType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="horizontalPositionUncertaintyType">
        <xs:annotation>
            <xs:documentation>The best estimate of the accuracy of a position.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="uncertaintyFixed" type="uncertaintyFixedType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="uncertaintyVariableFactor" type="uncertaintyVariableFactorType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="informationType">
        <xs:annotation>
            <xs:documentation>Textual information about the feature. The information may be provided as a string of text or as a file name of a single external text file that contains the text.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="fileLocator" type="fileLocatorType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="fileReference" type="fileReferenceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="headline" type="headlineType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="language" type="languageType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="text" type="textType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="landmarkDescriptionType">
        <xs:annotation>
            <xs:documentation>Textual description of selected landmarks that have significance in an area.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="limitsDescriptionType">
        <xs:annotation>
            <xs:documentation>Description of the area covered by the information specified.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="majorLightDescriptionType">
        <xs:annotation>
            <xs:documentation>A description of navigationally significant lights essential for marking landfalls, offshore dangers, shipping routes, port access channels or protection of the marine environment.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="markedByType">
        <xs:annotation>
            <xs:documentation>Description of the aids to navigation used to mark an area or object.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="onlineResourceType">
        <xs:annotation>
            <xs:documentation>Information about online sources from which a resource or data can be obtained.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="onlineResourceLinkageURL" type="onlineResourceLinkageURLType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="protocol" type="protocolType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="applicationProfile" type="applicationProfileType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="nameOfResource" type="nameOfResourceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="onlineResourceDescription" type="onlineResourceDescriptionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="onlineFunction" type="onlineFunctionType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="protocolRequest" type="protocolRequestType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="offshoreMarkDescriptionType">
        <xs:annotation>
            <xs:documentation>Description of aids to navigation or prominent marks located away from the shore.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="orientationType">
        <xs:annotation>
            <xs:documentation>(1) The angular distance measured from true north to the major axis of the feature. (2) In ECDIS, the mode in which information on the ECDIS is being presented. Typical modes include: north-up - as shown on a nautical chart, north is at the top of the display; Ships head-up - based on the actual heading of the ship, (e.g. Ships gyrocompass); course-up display - based on the course or route being taken.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="orientationUncertainty" type="orientationUncertaintyType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="orientationValue" type="orientationValueType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="scheduleByDayOfWeekType">
        <xs:annotation>
            <xs:documentation>The nature and timings of a daily schedule by days of the week.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="categoryOfSchedule" type="categoryOfScheduleType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="timeIntervalsByDayOfWeek" type="timeIntervalsByDayOfWeekType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="periodicDateRangeType">
        <xs:annotation>
            <xs:documentation>The active period of a recurring event or occurrence.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="dateStart" type="dateStartType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="dateEnd" type="dateEndType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="rxNCodeType">
        <xs:annotation>
            <xs:documentation>A summary of the impact of the most common types of regulation, restriction, recommendation and nautical information on a vessel.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="categoryOfRxN" type="categoryOfRxNType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="actionOrActivity" type="actionOrActivityType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="headline" type="headlineType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="spatialAccuracyType">
        <xs:annotation>
            <xs:documentation>Provides an indication of the vertical and horizontal positional uncertainty of bathymetric data, optionally within a specified date range.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="fixedDateRange" type="fixedDateRangeType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="horizontalPositionUncertainty" type="horizontalPositionUncertaintyType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="verticalUncertainty" type="verticalUncertaintyType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="surveyDateRangeType">
        <xs:annotation>
            <xs:documentation>The complex attribute describes the period of the hydrographic survey, as the time between its sub-attributes.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="dateStart" type="dateStartType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="dateEnd" type="dateEndType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="telecommunicationsType">
        <xs:annotation>
            <xs:documentation>A means or channel of communicating at a distance by electrical or electromagnetic means such as telegraphy, telephony, or broadcasting.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="categoryOfCommunicationPreference" type="categoryOfCommunicationPreferenceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="telecommunicationIdentifier" type="telecommunicationIdentifierType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="telecommunicationCarrier" type="telecommunicationCarrierType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="contactInstructions" type="contactInstructionsType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="telecommunicationService" type="telecommunicationServiceType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="scheduleByDayOfWeek" type="scheduleByDayOfWeekType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="textContentType">
        <xs:annotation>
            <xs:documentation>Textual material, or a pointer to a resource providing textual material. May be accompanied by basic information about its source and relationship to the source.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="categoryOfText" type="categoryOfTextType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="information" type="informationType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="onlineResource" type="onlineResourceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="source" type="sourceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="sourceType" type="sourceTypeType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="reportedDate" type="reportedDateType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="timeIntervalsByDayOfWeekType">
        <xs:annotation>
            <xs:documentation>The regular weekly operation times of a service or schedule.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="dayOfWeek" type="dayOfWeekType" minOccurs="0" maxOccurs="7"/>
            <xs:element name="dayOfWeekIsRange" type="dayOfWeekIsRangeType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="timeOfDayStart" type="timeOfDayStartType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="timeOfDayEnd" type="timeOfDayEndType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="usefulMarkDescriptionType">
        <xs:annotation>
            <xs:documentation>Description of Aids to Navigation or prominent marks which are usually clearly visible and identifiable enough to be used in determining location or direction.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="textContent" type="textContentType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="vesselsMeasurementsType">
        <xs:annotation>
            <xs:documentation>Values, discovered by measuring, that correspond to vessels characteristics.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="comparisonOperator" type="comparisonOperatorType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="vesselsCharacteristics" type="vesselsCharacteristicsType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="vesselsCharacteristicsValue" type="vesselsCharacteristicsValueType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="vesselsCharacteristicsUnit" type="vesselsCharacteristicsUnitType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="weatherResourceType">
        <xs:annotation>
            <xs:documentation>Links for relevant weather related information.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="onlineResource" type="onlineResourceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="dynamicResource" type="dynamicResourceType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="textContent" type="textContentType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="verticalUncertaintyType">
        <xs:annotation>
            <xs:documentation>The best estimate of the vertical accuracy of depths, heights, vertical distances and vertical clearances.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="uncertaintyFixed" type="uncertaintyFixedType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="uncertaintyVariableFactor" type="uncertaintyVariableFactorType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="InformationTypeType" abstract="true">
        <xs:annotation>
            <xs:documentation>Generalized information type which carries all the common attributes</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractInformationType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="featureName" type="featureNameType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="fixedDateRange" type="fixedDateRangeType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="periodicDateRange" type="periodicDateRangeType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="graphic" type="graphicType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="source" type="sourceType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="sourceType" type="sourceTypeType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="reportedDate" type="reportedDateType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="providesInformation" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AbstractRxNType" abstract="true">
        <xs:annotation>
            <xs:documentation>An abstract superclass for information types that encode rules, recommendations, and general information in text or graphic form.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="InformationTypeType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="categoryOfAuthority" type="categoryOfAuthorityType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="rxNCode" type="rxNCodeType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="textContent" type="textContentType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="isApplicableTo" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="theOrganisation" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AvailablePortServicesType">
        <xs:annotation>
            <xs:documentation>Services that are available for a given port.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="InformationTypeType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="firefightingService" type="firefightingServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="medicalService" type="medicalServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="repairService" type="repairServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="technicalPortService" type="technicalPortServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="shipSanitationControl" type="shipSanitationControlType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="transportConnection" type="transportConnectionType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="berthingAssistance" type="berthingAssistanceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="cargoService" type="cargoServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="securitySafetyEmergencyService" type="securitySafetyEmergencyServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="wasteDisposalService" type="wasteDisposalServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="supplyService" type="supplyServiceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="tugInformation" type="tugInformationType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="textContent" type="textContentType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApplicabilityType">
        <xs:annotation>
            <xs:documentation>Describes the relationship between vessel characteristics and: (i) the applicability of an associated information object or feature to the vessel; or, (ii) the use of a facility, place, or service by the vessel; or, (iii) passage of the vessel through an area.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="InformationTypeType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="inBallast" type="inBallastType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="categoryOfCargo" type="categoryOfCargoType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="categoryOfDangerousOrHazardousCargo" type="categoryOfDangerousOrHazardousCargoType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="categoryOfVessel" type="categoryOfVesselType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="categoryOfVesselRegistry" type="categoryOfVesselRegistryType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="logicalConnectives" type="logicalConnectivesType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="thicknessOfIceCapability" type="thicknessOfIceCapabilityType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="vesselPerformance" type="vesselPerformanceType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="information" type="informationType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="vesselsMeasurements" type="vesselsMeasurementsType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="theApplicableRxN" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="vslLocation" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AuthorityType">
        <xs:annotation>
            <xs:documentation>A person or organisation having political or administrative power and control.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="InformationTypeType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="categoryOfAuthority" type="categoryOfAuthorityType" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="textContent" type="textContentType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="theContactDetails" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="theInformation" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="theServiceHours" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ContactDetailsType">
        <xs:annotation>
            <xs:documentation>information on how to reach a person or organisation by postal, internet, telephone, telex and radio systems</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="InformationTypeType">
                <xs:sequence>
                    <!-- attributes -->
