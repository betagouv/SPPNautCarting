    <xs:simpleType name="categoryOfBerthLocationLabel">
        <xs:annotation>
            <xs:documentation>Classification of a berth according to the method of describing its location or extent.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"><xs:enumeration value="Wharf Reference Metre Mark"><xs:annotation><xs:documentation>1: A wharf or quay with reference position(s) given by one or more metre marks.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Wharf Reference Position"><xs:annotation><xs:documentation>2: A wharf or quay with reference position(s) given by one or more point or points in geographic coordinates.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Pier (Jetty)"><xs:annotation><xs:documentation>3: A long, narrow structure extending into the water to afford a berthing place for vessels, to serve as a promenade, etc.</xs:documentation></xs:annotation></xs:enumeration>
            <xs:enumeration value="Conventional Mooring"><xs:annotation><xs:documentation>4: Mooring using the vessel's anchors and buoys to secure the vessel at multiple points.</xs:documentation></xs:annotation></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
