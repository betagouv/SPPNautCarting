	<xs:complexType name="bearingInformationType">
		<xs:annotation>
			<xs:documentation>A bearing is the direction one object is from another object.
At least one of the sub-attributes must be present.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cardinalDirection" type="cardinalDirectionType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="distance" type="NonNegativeDecimal" minOccurs="0" maxOccurs="1"><xs:annotation><xs:documentation>A numeric measure of the spatial separation between two locations.</xs:documentation></xs:annotation></xs:element>
			<xs:element name="information" type="informationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="orientation" type="orientationType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="sectorBearing" type="xs:decimal" minOccurs="0" maxOccurs="2"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="orientationType">
		<xs:sequence>
			<xs:element name="orientationUncertainty" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="orientationValue" type="Decimal0.0To360.0" minOccurs="1" maxOccurs="1" nillable="true"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="restrictionType">
		<xs:annotation>
			<xs:documentation>The official legal statute of each kind of restricted area.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="anchoring prohibited">
				<xs:annotation>
					<xs:documentation>An area within which anchoring is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="anchoring restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which anchoring is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fishing prohibited">
				<xs:annotation>
					<xs:documentation>An area within which fishing is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fishing restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which fishing is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="trawling prohibited">
				<xs:annotation>
					<xs:documentation>An area within which trawling is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="trawling restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which trawling is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="entry prohibited">
				<xs:annotation>
					<xs:documentation>An area within which navigation and/or anchoring is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="entry restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which navigation is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dredging prohibited">
				<xs:annotation>
					<xs:documentation>An area within which dredging is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dredging restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which dredging is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="diving prohibited">
				<xs:annotation>
					<xs:documentation>An area within which diving is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="diving restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which diving is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="no wake">
				<xs:annotation>
					<xs:documentation>Mariners must adjust the speed of their vessels to reduce the wave or wash which may cause erosion or disturb moored vessels.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="area to be avoided">
				<xs:annotation>
					<xs:documentation>An IMO declared routeing measure comprising an area within defined limits in which either navigation is particularly hazardous or it is exceptionally important to avoid casualties and which should be avoided by all ships, or certain classes of ships.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="construction prohibited">
				<xs:annotation>
					<xs:documentation>The erection of permanent or temporary fixed structures or artificial islands is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="discharging prohibited">
				<xs:annotation>
					<xs:documentation>An area in which discharging or dumping is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="discharging restricted">
				<xs:annotation>
					<xs:documentation>A specified area designated by appropriate authority, within which discharging or dumping is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="industrial or mineral exploration/development prohibited">
				<xs:annotation>
					<xs:documentation>An area in which industrial or mineral exploration and development are prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="industrial or mineral exploration/development restricted">
				<xs:annotation>
					<xs:documentation>A specified area designated by appropriate authority, within which industrial or mineral exploration and development is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="drilling prohibited">
				<xs:annotation>
					<xs:documentation>An area in which excavating a hole on the seabed with a drill is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="drilling restricted">
				<xs:annotation>
					<xs:documentation>A specified area designated by appropriate authority, within which excavating a hole on the seabed with a drill is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="removal of historical artifacts prohibited">
				<xs:annotation>
					<xs:documentation>An area in which the removal of historical artifacts is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cargo transhipment (lightering) prohibited">
				<xs:annotation>
					<xs:documentation>An area in which cargo transhipment (lightering) is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dragging prohibited">
				<xs:annotation>
					<xs:documentation>An area in which the dragging or anything along the seabed, e.g., bottom trawling, is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="stopping prohibited">
				<xs:annotation>
					<xs:documentation>An area in which a vessel is prohibited from stopping.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="landing prohibited">
				<xs:annotation>
					<xs:documentation>An area in which landing is prohibited.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="speed restricted">
				<xs:annotation>
					<xs:documentation>An area in which speed is restricted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="overtaking prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which overtaking is generally prohibited</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="overtaking of convoys by convoys prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which overtaking between convoys prohibited</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="passing or overtaking prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which passing or overtaking is generally prohibited</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="berthing prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which vessels, assemblies of floating material or floating establishments may not berth.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="berthing restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which berthing is restricted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="making fast prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which vessels, assemblies of floating material or floating establishments may not make fast to the bank.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="making fast restricted">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which making fast to the bank is restricted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="turning prohibited">
				<xs:annotation>
					<xs:documentation>a specified area designated by appropriate authority, within which all turning is generally prohibited</xs:documentation>
				</xs:annotation>
