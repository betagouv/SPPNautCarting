	<!--<xs:element name="S100_ProductSpecification" type="S100_ProductSpecification"/>-->
	<xs:complexType name="S100_ProductSpecification">
		<xs:annotation>
			<xs:documentation>The Product Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of the product specification used to create the datasets.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="S100_SupportFileSpecification">
		<xs:annotation>
			<xs:documentation>The Support File Specification contains the information needed to build the specified product</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of the specification used to create the support file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="S100_SupportFilePurpose">
		<xs:annotation>
			<xs:documentation>The reason for inclusion of the support file in this exchange set</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="new">
				<xs:annotation>
					<xs:documentation>A file which is new. Signifies a new file.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="replacement">
				<xs:annotation>
					<xs:documentation>A file which replaces an existing file. Signifies a replacement for a file of the same name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="deletion">
				<xs:annotation>
					<xs:documentation>Deletes an existing file. Signifies deletion of a file of that name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--<xs:element name="S100_SupportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata"/>-->
	<xs:complexType name="S100_SupportFileDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>Metadata about the individual support files in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>File name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileLocation" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Full path from the exchange set root directory. Path relative to the root directory of the exchange set. The location of the file after the exchange set is unpacked into directory &lt;EXCH_ROOT&gt; will be &lt;EXCH_ROOT&gt;/&lt;filePath&gt;/&lt;filename&gt;</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="S100_SupportFilePurpose" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the dataset has been issued. e.g. new, re-issue, new edition, update etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of the dataset. When a data set is initially created, the edition number 1 is assigned to it. The edition number is increased by 1 at each new edition. Edition number remains the same for a re-issue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supportFileSpecification" type="S100_SupportFileSpecification" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The product specification used to create this file. Optional if datatype is Text. This is a deviation from the model in Part 4a of S-100 edition 2.0.0.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataType" type="S100_SupportFileFormat" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The encoding format of the dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherDataTypeDescription" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Encoding format other than those listed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional comment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="checkSum" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The Cyclic Redundancy Checksum for the file.</xs:documentation>
				</xs:annotation>
			</xs:element>-->
			<xs:element name="digitalSignatureReference" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file.  Reference to the appropriate digital signature algorithm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="S100_SupportFile" type="S100_SupportFile" minOccurs="1" maxOccurs="1"/>-->
		</xs:sequence>
	</xs:complexType>
	<!--<xs:element name="S100_CatalogueIdentifier" type="S100_CatalogueIdentifier"/>-->
	<xs:complexType name="S100_CatalogueIdentifier">
		<xs:annotation>
			<xs:documentation>An exchange catalogue contains the discovery metadata about the exchange datasets and support files</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Uniquely identifies this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Creation date of the exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!--<xs:element name="S100_CataloguePointofContact" type="S100_CataloguePointofContact"/>-->
	<xs:complexType name="S100_CataloguePointofContact">
		<xs:annotation>
			<xs:documentation>Contact details of the issuer of this exchange catalogue</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="organization" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The organization distributing this exchange catalogue. This could be an individual producer, value added reseller, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phone" type="S100_Phone_Type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="address" type="S100_Address_Type" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!--<xs:element name="S100_DatasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata"/>-->
	<xs:complexType name="S100_DatasetDiscoveryMetadata">
		<xs:annotation>
			<xs:documentation>S-100_DatasetDiscoveryMetadata. Metadata about the individual datasets in the exchange catalogue.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dataset file name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="filePath" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Full path from the exchange set root directory</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Short description giving the area or location covered by the dataset. e.g. a harbour or port name, between two named locations etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataProtection" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the data is encrypted. 0 indicates an unencrypted dataset; 1 indicates an encrypted dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="protectionScheme" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Eg S-63</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignature" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the data has a digital signature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file. Specifies the algorithm used to compute digitalSignatureValue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature. The value resulting from application of digitalSignatureReference</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="copyright" type="S100_RestrictionCode_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the dataset is copyrighted</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classification" type="S100_ClassificationCode_PropertyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates the security classification of the dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="purpose" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The purpose for which the dataset has been issued. e.g. new, re-issue, new edition, update etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specificUsage" type="S100_Usage_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The use for which the dataset is intended. e.g. in the case of ENCs this would be a navigation purpose classification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The edition number of the dataset. When a data set is initially created, the edition number 1 is assigned to it. The edition number is increased by 1 at each new edition. Edition number remains the same for a re-issue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Update number assigned to the dataset and increased by one for each subsequent update. Update number 0 is assigned to a new dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateApplicationDate" type="gco:Date_Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This date is only used for the base cell files (i.e. new data sets, re-issue and new edition), not update cell files. All updates dated on or before this date must have been applied by the producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Date on which the data was made available by the data producer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The product specification used to create this dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="producingAgency" type="S100_ResponsibleParty_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ProducingAgency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="optimumDisplayScale" type="xs:integer" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The modulus of the display scale. Example: A scale of 1:25000 is encoded as 25000.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maximumDisplayScale" type="xs:integer" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The maximum scale with which the data is displayed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="minimumDisplayScale" type="xs:integer" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The minimum scale with which the data is displayed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="horizontalDatumReference" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Reference to the register from which the horizontal datum value is taken. e.g.EPSG</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="horizontalDatumValue" type="xs:integer" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Horizontal Datum of the entire dataset. e.g. EPSG 4326</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verticalDatum" type="S100_VerticalAndSoundingDatum" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Vertical Datum of the entire dataset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="soundingDatum" type="S100_VerticalAndSoundingDatum" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Sounding Datum of the entire dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataType" type="S100_DataFormat" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The encoding format of the dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherDataTypeDescription" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Encoding format other than those listed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataTypeVersion" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version number of the dataType.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataCoverage" type="S100_DataCoverage_Type" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Provides information about data coverages within the dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="comment" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional comment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="checkSum" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The Cyclic Redundancy Checksum for the file.</xs:documentation>
				</xs:annotation>
			</xs:element>-->
			<xs:element name="layerID" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identifies other layers with which the data set is intended to be used or portrayed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="S100_19115DatasetMetadata" type="S100_19115DatasetMetadata" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>ISO 1915 metadata. Structure TBD</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="S100_SupportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="S100_DataFormat">
		<xs:annotation>
			<xs:documentation>The encoding format.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ISO/IEC 8211 ASCII"/>
			<xs:enumeration value="ISO/IEC 8211 BINARY"/>
			<xs:enumeration value="GML"/>
			<xs:enumeration value="HDF5"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
		<!-- UML model of 10JUL2015 corrected to conform to Ed. 2.0.0 4a-D.2.8 by adding IEC in 2 literals -->
	</xs:simpleType>
	<xs:simpleType name="S100_SupportFileFormat">
		<xs:annotation>
			<xs:documentation>The format used in the support file. Literals as defined in S-100 3.0.0 should be modified.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<!--<xs:enumeration value="Text"><xs:annotation><xs:documentation>Changed to ASCII in Ed. 3.0.0, but ASCII != UTF8, which would leave nothing appropriate for UTF8 or Latin-1 text files.</xs:documentation></xs:annotation></xs:enumeration>-->
			<xs:enumeration value="JPEG2000"/>
			<xs:enumeration value="HTML"/>
			<xs:enumeration value="XML"/>
			<xs:enumeration value="XSLT"/>
			<xs:enumeration value="VIDEO"/>
			<xs:enumeration value="other"/>
			<!--<xs:enumeration value="Other"/>--><!-- UML diagram in Figure 4a-d-4 has lower case "other" -->
			<xs:enumeration value="ASCII"><xs:annotation><xs:documentation>As in Ed. 3.0.0 but needs fix, UTF8 is not ASCII</xs:documentation></xs:annotation></xs:enumeration>
			<!--<xs:enumeration value="XHTML"/>-->
			<xs:enumeration value="TIFF"/>
			<xs:enumeration value="PDF/A or UA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="S100_CatalogueScope_Type">
		<xs:annotation>
			<xs:documentation>Definition required</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="featureCatalogue"/>
			<xs:enumeration value="portrayalCatalogue"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="S100_ExchangeCatalogue" type="S100_ExchangeCatalogue"/>
	<xs:complexType name="S100_ExchangeCatalogue">
		<xs:annotation>
			<xs:documentation>Each exchange set has a single S-100_ExchangeCatalogue which contains meta information for the data and support files in the exchange set. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="S100_CatalogueIdentifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Uniquely identifies this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="S100_CataloguePointofContact" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the issuer of this exchange catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the product specifications used for the datasets contained in the exchange catalogue. Conditional on all the datasets using the same product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataLanguage" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details about the language.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueName" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Catalogue filename. In S-101 it would be CATLOG.101</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueDescription" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of what the exchange catalogue contains</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exchangeCatalogueComment" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Any additional Information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compressionFlag" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Is the data compressed? Yes or No</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="algorithmMethod" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Type of compression algorithm. Eg. RAR or ZIP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sourceMedia" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Distribution media</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="replacedData" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>If a data file is canceled is it replaced by another data file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataReplacement" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Cell name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="S100_ExchangeSet" type="S100_ExchangeSet" minOccurs="1" maxOccurs="1"/>-->
			<xs:element name="datasetDiscoveryMetadata" type="S100_DatasetDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="supportFileDiscoveryMetadata" type="S100_SupportFileDiscoveryMetadata" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="S100_CatalogueMetadata" type="S100_CatalogueMetadata" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation><xs:documentation>Metadata for feature and portrayal catalogues (Fig. 4a-D-2 suggests their metadata should be in the exchange catalogue, 4a-D-3 suggests their metadata might be separate files in the exchange set)</xs:documentation></xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="S100_CatalogueMetadata" type="S100_CatalogueMetadata"/>
	<xs:complexType name="S100_CatalogueMetadata">
		<xs:annotation><xs:documentation>Definition required</xs:documentation></xs:annotation>
		<xs:sequence>
			<xs:element name="fileName" type="xs:string" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The name for the catalogue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileLocation" type="xs:string" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Full location from the exchange set root directory</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="scope" type="S100_CatalogueScope_Type" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Subject domain of the catalogue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="xs:string" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The version number of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="gco:Date_Type" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The version date of the product specification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productSpecification" type="S100_ProductSpecification" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The product specification used to create this dataset.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureReference" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Digital Signature of the file.  Reference to the appropriate digital signature algorithm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="digitalSignatureValue" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Value derived from the digital signature</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
