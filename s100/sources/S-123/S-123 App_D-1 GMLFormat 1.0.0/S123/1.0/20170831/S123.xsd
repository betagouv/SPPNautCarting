			<xs:enumeration value="IMDG Code Class 8">
				<xs:annotation>
					<xs:documentation>Corrosive substances </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 9">
