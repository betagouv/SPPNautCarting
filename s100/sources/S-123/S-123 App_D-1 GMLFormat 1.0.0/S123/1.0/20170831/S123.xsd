					<xs:enumeration value="voice">
						<xs:annotation>
							<xs:documentation>The transfer or exchange of information by using sounds that are being made by mouth and throat when speaking</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="facsimile">
						<xs:annotation>
							<xs:documentation>a system of transmitting and reproducing graphic matter (as printing or still pictures) by means of signals sent over telephone lines</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="sms">
						<xs:annotation>
							<xs:documentation>Short Message Service, a form of text messaging communication on phones and mobile phones</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="data">
						<xs:annotation>
							<xs:documentation>facts or information used usually to calculate, analyze, or plan something</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="streamedData">
						<xs:annotation>
							<xs:documentation>Streamed data is data that that is constantly received by and presented to an end-user while being delivered by a provider.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="telex">
						<xs:annotation>
							<xs:documentation>a system of communication in which messages are sent over long distances by using a telephone system and are printed by using a special machine (called a teletypewriter)</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="telegraph">
						<xs:annotation>
							<xs:documentation>an apparatus, system, or process for communication at a distance by electric transmission over wire</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="email">
						<xs:annotation>
							<xs:documentation>Messages and other data exchanged between individuals using computers in a network.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="other: [a-zA-Z0-9]+( [a-zA-Z0-9]+)*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<xs:simpleType name="actionOrActivityType">
		<xs:annotation>
			<xs:documentation>The action or activity of a vessel</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="navigating with a pilot">
						<xs:annotation>
							<xs:documentation>Carrying a qualified pilot as part of the vessel navigation team.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="entering port">
						<xs:annotation>
							<xs:documentation>navigating a vessel into a port</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="leaving port">
						<xs:annotation>
							<xs:documentation>Navigating a vessel out of a port.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="berthing">
						<xs:annotation>
							<xs:documentation>Attaching a vessel to a wharf or jetty.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="slipping">
						<xs:annotation>
							<xs:documentation>Detaching a vessel from a wharf or jetty.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="anchoring">
						<xs:annotation>
							<xs:documentation>Attaching a vessel to the seabed by means of an anchor and cable.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="weighing anchor">
						<xs:annotation>
							<xs:documentation>Detaching a vessel from the seabed by recovering an anchor and cable.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="transiting">
						<xs:annotation>
							<xs:documentation>Navigating a vessel along a route or through a narrow gap, such as under a bridge or through a lock.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="overtaking">
						<xs:annotation>
							<xs:documentation>Navigating a vessel past another traveling broadly in the same direction.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="reporting">
						<xs:annotation>
							<xs:documentation>Providing details such as the name, location or intentions of a vessel</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="working cargo">
						<xs:annotation>
							<xs:documentation>Loading or unloading cargo</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="landing">
						<xs:annotation>
							<xs:documentation>Placing crew or passengers on shore.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="diving">
						<xs:annotation>
							<xs:documentation>Placing a swimmer with an air supply below the sea surface.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="fishing">
						<xs:annotation>
							<xs:documentation>Hunting or catching fish.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="discharging overboard">
						<xs:annotation>
							<xs:documentation>Releasing anything into the sea; often ballast water; or spoil from dredging elsewhere.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="passing">
						<xs:annotation>
							<xs:documentation>Navigating a vessel past another traveling broadly in the opposite direction.</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="other: [a-zA-Z0-9]+( [a-zA-Z0-9]+)*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="categoryOfRxNType">
		<xs:annotation>
			<xs:documentation>The principal subject matter of regulations, restrictions, recommendations or nautical information</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="navigation">
						<xs:annotation>
							<xs:documentation>Pertaining to navigation</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="communication">
						<xs:annotation>
							<xs:documentation>Pertaining to communications</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="environmental protection">
						<xs:annotation>
							<xs:documentation>Pertaining to environmental protection </xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="wildlife protection">
						<xs:annotation>
							<xs:documentation>Pertaining to wildlife protection</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="security">
						<xs:annotation>
							<xs:documentation>Pertaining to security </xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="customs">
						<xs:annotation>
							<xs:documentation>Pertaining to customs</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="cargo operation">
						<xs:annotation>
							<xs:documentation>Pertaining to cargo operations</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="refuge">
						<xs:annotation>
							<xs:documentation>Pertaining to a place of safety or refuge</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="natural resources or exploitation">
						<xs:annotation>
							<xs:documentation>Pertaining to natural resources or exploitation</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="port">
						<xs:annotation>
							<xs:documentation>Pertaining to a port</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="finance">
						<xs:annotation>
							<xs:documentation>Pertaining to finance</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="agriculture">
						<xs:annotation>
							<xs:documentation>Pertaining to agriculture</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="other: [a-zA-Z0-9]+( [a-zA-Z0-9]+)*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>


	<!-- ================================================= -->
	<!-- substitution head for meta-features               -->
	<!-- distinguished from ordinary features to allow     -->
	<!-- meta-features to be distinguished from ordinary   -->
	<!-- geo-features                                      -->
	<!-- ================================================= -->
	<xs:element name="MetaFeatureType" type="MetaFeatureType" abstract="true" substitutionGroup="gml:AbstractFeature"/>
	<xs:complexType name="MetaFeatureType" abstract="true">
		<xs:annotation>
			<xs:documentation>Generalized feature type which carry all the common attributes</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="S100:AbstractFeatureType"/>
		</xs:complexContent>
	</xs:complexType>
	<!-- ============================================= -->
	<!-- types and elements for meta features		   -->
	<!-- ============================================= -->
	<xs:simpleType name="categoryOfTemporalVariationType">
		<xs:annotation>
			<xs:documentation>An assessment of the likelihood of change within an area since last survey.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="extreme event">
				<xs:annotation>
					<xs:documentation>NIPWG: No new hydrographic survey conducted after an event (e.g., hurricane, earthquake, volcanic eruption, landslide, etc.) which is considered likely to have resulted in significant change at the location.</xs:documentation>
					<xs:documentation>In GI Registry: Definition: No new hydrographic survey conducted after an event (e.g. hurricane, earthquake, volcanic eruption, landslide, etc), which is considered likely to have changed the seafloor significantly.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="likely to change">
				<xs:annotation>
					<xs:documentation>NIPWG: Continuous or frequent change (e.g., river siltation, sand waves, seasonal storms, construction, etc.)
						GI Registry:
						Continuous or frequent change (e.g. river siltation, sand waves, seasonal storms, ice bergs, etc).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="likely to change but significant shoaling not expected">
				<xs:annotation>
					<xs:documentation>GI Registry: Likely to change but significant shoaling no expected
						[Proposed draft 6/27/2014: Recommended that SNPWG not use this enumerate - ref. E.M. email 6/27/2014.]</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unlikely to change">
				<xs:annotation>
					<xs:documentation>NIPWG: Significant change at the location is not expected.</xs:documentation>
					<xs:documentation>GI Registry: Significant change to the seafloor is not expected.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unassessed">
				<xs:annotation>
					<xs:documentation>Temporal variation not assessed or cannot be determined</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dataAssessmentType">
		<xs:annotation>
			<xs:documentation>The categorisation of the assessment level of bathymetric data for an area.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="assessed">
				<xs:annotation>
					<xs:documentation>The quality of the bathymetric data has been assessed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="oceanic">
				<xs:annotation>
					<xs:documentation>The quality of oceanic bathymetric data has been assessed or is not required.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unassessed">
				<xs:annotation>
					<xs:documentation>The quality of the bathymetric data has yet to be assessed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QUAPOSType">
		<xs:annotation>
			<xs:documentation>Definition required</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="surveyed">
				<xs:annotation>
					<xs:documentation>The position(s) was(were) determined by the operation of making measurements for determining the relative position of points on, above, or beneath the earth's surface. Survey implies a regular, controlled, survey of any date.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unsurveyed">
				<xs:annotation>
					<xs:documentation>Survey data does not exist or is very poor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="inadequately surveyed">
				<xs:annotation>
					<xs:documentation>Position data is of very poor quality</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate">
				<xs:annotation>
					<xs:documentation>A position that is considered to be less than third-order accuracy, but is generally considered to be within 30.5 meters of its correct geographic location. Also, may apply to a feature whose position does not remain fixed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="position doubtful">
				<xs:annotation>
					<xs:documentation>A feature whose position has been reported but which is considered to be doubtful</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unreliable">
				<xs:annotation>
					<xs:documentation>A feature's position obtained from questionable or unreliable data.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reported (not surveyed)">
				<xs:annotation>
					<xs:documentation>A feature whose position has been reported and its position confirmed by some means other than a formal survey such as an independent report of the same feature.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reported (not confirmed">
				<xs:annotation>
					<xs:documentation>A feature whose position has been reported and its position has not been confirmed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="estimated">
				<xs:annotation>
					<xs:documentation>The most probable position of a feature determined from incomplete data or data of questionable accuracy.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="precisely known">
				<xs:annotation>
					<xs:documentation>A position that is of a known value, such as the position of an anchor berth or other defined feature.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="calculated">
				<xs:annotation>
					<xs:documentation>A position that is computed from data.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="horizontalPositionalUncertainty">
		<xs:annotation>
			<xs:documentation>Definition required</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="uncertaintyFixed" type="xs:decimal" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>Definition required</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="uncertaintyVariable" type="xs:decimal" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Definition required</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="surveyDateRangeType">
		<xs:annotation>
			<xs:documentation>The complex attribute describes the period of the hydrographic survey, as the time between its sub-attributes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dateStart" type="S100_TruncatedDate" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The start date or time of the interval.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateEnd" type="S100_TruncatedDate" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The end date or time of the interval.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="DataQuality" type="DataQualityType" abstract="true" substitutionGroup="MetaFeatureType"/>
	<xs:complexType name="DataQualityType" abstract="true">
		<xs:annotation>
			<xs:documentation>Abstract feature type for data quality meta-features</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="MetaFeatureType">
				<xs:sequence>
					<xs:element name="information" type="informationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Use of attribute information is discouraged for nautical publications data quality meta-features.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!--<xs:element name="geometry" type="GM_OrientableSurface" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Permitted spatial primitives</xs:documentation>
						</xs:annotation>
					</xs:element>-->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="QualityOfTemporalVariationType" type="QualityOfTemporalVariationType" abstract="true" substitutionGroup="MetaFeatureType"/>
	<xs:complexType name="QualityOfTemporalVariationType" abstract="true">
		<xs:annotation>
			<xs:documentation>Abstract type for meta-feature which can describe temporal variation.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="DataQualityType">
				<xs:sequence>
					<xs:element name="categoryOfTemporalVariation" type="categoryOfTemporalVariationType" minOccurs="1" maxOccurs="1" nillable="true"/>
					<xs:element name="dataAssessment" type="dataAssessmentType" minOccurs="1" maxOccurs="1" nillable="true">
						<xs:annotation>
							<xs:documentation>The categorisation of the assessment level of bathymetric data for an area.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataCoverage" type="DataCoverageType" substitutionGroup="MetaFeatureType"/>
	<xs:complexType name="DataCoverageType">
		<xs:annotation>
			<xs:documentation>A geographical area that describes the coverage and extent of spatial types.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="MetaFeatureType">
				<xs:sequence>
					<xs:element name="maximumDisplayScale" type="xs:positiveInteger" minOccurs="1" maxOccurs="1" nillable="true">
						<xs:annotation>
							<xs:documentation>The largest intended viewing scale for the data. Not nillable(?)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="minimumDisplayScale" type="xs:positiveInteger" minOccurs="1" maxOccurs="1" nillable="true">
						<xs:annotation>
							<xs:documentation>The smallest intended viewing scale for the data. Not nillable(?)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="geometry" type="S100EXT:SurfacePropertyType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="QualityOfNonBathymetricData" type="QualityOfNonBathymetricDataType" substitutionGroup="MetaFeatureType"/>
	<xs:complexType name="QualityOfNonBathymetricDataType">
		<xs:annotation>
			<xs:documentation>An area within which a uniform assessment of the quality of the non-bathymetric data exists. (Adapted from S-57 Edition 3.1, Appendix A – Chapter 1, Page 1.208, November 2000).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="QualityOfTemporalVariationType">
				<xs:sequence>
					<xs:element name="horizontalDistanceUncertainty" type="xs:string" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The best estimate of the horizontal accuracy of horizontal clearances and distances.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="horizontalPositionalUncertainty" type="horizontalPositionalUncertainty" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The best estimate of the accuracy of a position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="directionUncertainty" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The best estimate of the accuracy of a bearing</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="sourceIndication" type="sourceIndicationType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="surveyDateRange" type="surveyDateRangeType" minOccurs="0" maxOccurs="1"/>
					<!--<xs:element name="geometry" type="GM_Surface" minOccurs="1" maxOccurs="unbounded"/>-->
					<xs:element name="geometry" type="S100EXT:SurfacePropertyType" minOccurs="1" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="SpatialQuality" type="SpatialQuality"/>
	<xs:complexType name="SpatialQuality">
		<xs:complexContent>
			<xs:extension base="InformationTypeType">
				<xs:sequence>
					<xs:element name="categoryOfTemporalVariation" type="categoryOfTemporalVariationType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="horizontalPositionalUncertainty" type="horizontalPositionalUncertainty" minOccurs="0" maxOccurs="1"/>
					<xs:element name="qualityOfHorizontalMeasurement" type="QUAPOSType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="SpatialQualityPoints" type="SpatialQualityPoints"/>
	<xs:complexType name="SpatialQualityPoints">
		<xs:annotation>
			<xs:documentation>Definition required</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SpatialQuality">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- ==================================================== -->
	<!-- types and elements specific to cartographic features -->
	<!-- ==================================================== -->
	<xs:simpleType name="textJustificationType">
		<xs:annotation>
			<xs:documentation>The anchor point of a text string.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="left">
				<xs:annotation>
					<xs:documentation>The anchor point is at the start of the text string.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="centred">
				<xs:annotation>
					<xs:documentation>The anchor point is at the centre of the text string.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="right">
				<xs:annotation>
					<xs:documentation>The anchor point is at the end of the text string.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="TextPlacement" type="TextPlacement"/>
	<xs:complexType name="TextPlacement">
		<xs:annotation>
			<xs:documentation>The Text Placement feature is used in association with the FeatureName attribute or a light description to optimise text positioning in ECDIS.</xs:documentation>
			<xs:documentation>Remarks: - The Text Placement feature is used by the ECDIS to position the associated text, which has been populated using an attribute(s) for the related feature. This attribute is identified by populating the attribute text type. Alternatively, the text to be displayed may be encoded using the attribute text.
				- Only one of the attributes text or text type are allowable for each instance of Text Placement.
				- Text Placement should only be associated with features of type point, and used in areas where it is important that text clear navigationally relevant areas, e.g. shipping channels and dredged areas.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="flipBearing" type="xs:decimal" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The bearing at which text is re-located to the opposite side of a feature when screen display is oriented away from true north.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="scaleMinimum" type="xs:integer" minOccurs="0" maxOccurs="1"/>
			<xs:element name="textJustification" type="textJustificationType" minOccurs="1" maxOccurs="1" nillable="true">
				<xs:annotation>
					<xs:documentation>The anchor point of a text string.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A non-formatted digital text string.</xs:documentation>
					<xs:documentation>Remarks: The attribute should be used, for example, to hold the information that is shown on paper charts by short cautionary and explanatory notes. Therefore text populated in text must not exceed 300 characters.</xs:documentation>
					<xs:documentation>Text may be in English or in a national language defined by the attribute language. No formatting of text is possible within the sub-attribute text. If formatted text, or text strings exceeding 300 characters, is required, then the attribute file reference must be used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="textType" type="textTypeType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="identifies" type="gml:ReferenceType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Association to feature for which the text is placed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geometry" type="GM_Point" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="textTypeType">
		<xs:annotation>
			<xs:documentation>The attribute from which a text string is derived.</xs:documentation>
			<xs:documentation>(S-122/123) Remark: S-122/123 do not include light features and therefore listed value '2: light characteristic' is omitted from the S-122 application schema.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="feature name">
				<xs:annotation>
					<xs:documentation>Definition required</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<!-- ==================================================== -->
	<!-- types and elements specific to fuzzy area modeling   -->
	<!-- ==================================================== -->
	<xs:simpleType name="informationConfidenceType">
		<xs:annotation>
			<xs:documentation>The likelihood that a vessel will experience the phenomenon described by a feature, or that the service described by the feature will be available.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="virtuallyCertain">
				<xs:annotation>
					<xs:documentation>Virtually certain to be experienced by (or available to) an individual vessel; will be experienced by nearly all vessels.  (FAA, adapted.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="highLikelihood">
				<xs:annotation>
					<xs:documentation>Frequently experienced by (or available to) an individual vessel; experienced by a majority of vessels. (FAA, adapted.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mediumLikelihood">
				<xs:annotation>
					<xs:documentation>Occasionally experienced by (or available to) an individual vessel; experienced by (or available to) about half of all vessels. (FAA, adapted.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lowLikelihood">
				<xs:annotation>
					<xs:documentation>Unlikely, but sometimes (rarely) experienced by (or available to) an individual vessel; experienced by (or available to) a minority of vessels). (FAA, adapted.)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ForecastAreaAggregate" type="ForecastAreaAggregateType" substitutionGroup="FeatureType"/>
	<xs:complexType name="ForecastAreaAggregateType">
		<xs:annotation>
			<xs:documentation>Aggregation of areas where forecasts and warnings broadcasted for a Weather forecast and warning area may be available with differing levels of reliability.</xs:documentation>
			<xs:documentation>consistsOf removed  from this type because duplicating the element of the same name in supertype FuzzyAreaAggregate violates XML schema rules</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FuzzyAreaAggregateType">
				<xs:sequence>
					<!--<xs:element name="consistsOf" type="gml:ReferenceType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A pointer to the aggregate in a whole-part relationship.</xs:documentation>
						</xs:annotation>
					</xs:element>--> </xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="FuzzyAreaAggregate" type="FuzzyAreaAggregateType" substitutionGroup="FeatureType" abstract="true"/>
	<xs:complexType name="FuzzyAreaAggregateType" abstract="true">
		<xs:annotation>
			<xs:documentation>Aggregation of a geographic feature describing a service or phenomenon with zones of different confidence about the availability of the service, occurrence of the phenomenon, or applicability of the information described by the geographic feature.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="consistsOf" type="gml:ReferenceType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A pointer to the aggregate in a whole-part relationship.</xs:documentation>
							<xs:documentation>Used for both the IndeterminateZone and core feature associations (same role name used for both)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="IndeterminateZone" type="IndeterminateZoneType" substitutionGroup="FeatureType"/>
	<xs:complexType name="IndeterminateZoneType">
		<xs:annotation>
			<xs:documentation>A region in which the perception of a phenomenon or the availability of a service is known only to a specified level of confidence.</xs:documentation>
			<xs:documentation>Remark: IndeterminateZone features associated to the same fuzzy area aggregate must not overlap.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="informationConfidence" type="informationConfidenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="componentOf" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="geometry" type="GM_Surface" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Fuzzy region spatial attribute</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="RadioServiceAreaAggregate" type="RadioServiceAreaAggregateType" substitutionGroup="FeatureType"/>
	<xs:complexType name="RadioServiceAreaAggregateType">
		<xs:annotation>
			<xs:documentation>Aggregation of areas where radio services from a single radio service are available to different levels of reliability</xs:documentation>
			<xs:documentation>consistsOf removed  from this type because duplicating the element of the same name in supertype FuzzyAreaAggregate violates XML schema rules</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FuzzyAreaAggregateType">
				<xs:sequence>
					<!--<xs:element name="consistsOf" type="gml:ReferenceType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A pointer to the aggregate in a whole-part relationship.</xs:documentation>
						</xs:annotation>
					</xs:element>--> </xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- ============================================= -->
	<!-- types and elements for the dataset definition -->
	<!-- ============================================= -->
	<xs:complexType name="DatasetType">
		<xs:annotation>
			<xs:documentation>Dataset element for MPA dataset as "GML document"</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="DatasetIdentificationInformation" type="S100:DataSetIdentificationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Dataset identification information</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DatasetStructureInformation" type="S100:DataSetStructureInformationType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Dataset structure information</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="S100:Geometry" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Allows spatial objects to be located outside feature objects (for references, and compatibility with ISO 8211 encoding)</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="imember" minOccurs="0" maxOccurs="unbounded" type="IMemberType">
							<xs:annotation>
								<xs:documentation>intended for S100 information types. Extension of GML practice, not addressed by ISO 19136.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="member" minOccurs="0" maxOccurs="unbounded" type="MemberType">
							<xs:annotation>
								<xs:documentation>intended for technical GML 3.2 requirement for making the dataset a "GML document" and clause 21.3 of the OGC GML standard</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- treatment of S-100 Information types is provisional, because GML does not have the concept, and they must be modeled as AbstractGML -->
	<xs:complexType name="MemberType">
		<xs:annotation>
			<xs:documentation>dataset member</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureMemberType">
				<xs:sequence>
					<xs:element ref="gml:AbstractFeature"/>
				</xs:sequence>
				<xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="IMemberType">
		<xs:annotation>
			<xs:documentation>dataset member S-100 infotmation types</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureMemberType">
				<xs:sequence>
					<xs:element ref="InformationType"/>
				</xs:sequence>
				<xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--<xs:complexType name="GenericFeatureType">
		<xs:complexContent>
			<xs:extension base="S100:AbstractFeatureType">
				<xs:sequence>
					<xs:any namespace="##local" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>-->

	<xs:element name="Dataset" type="DatasetType"/>
</xs:schema>
